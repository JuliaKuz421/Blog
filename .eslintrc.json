{
    "extends": "airbnb",

    "parser": "@babel/eslint-parser",

    "parserOptions": {
    "requireConfigFile": false
    },

     "rules": {
        "no-unused-vars": "off",
        "semi": "off",
        "indent": ["warn", 2],
        "arrow-parens": "off",
        "eol-last": "off",
        "arrow-body-style": "off",
        "import/extensions": "off",
        "no-undef": "off",
        "import/order": "off",
        "import/no-named-as-default": "off",
        "import/no-named-as-default-member": "off"
     }
}

